<div class="row">
  <% @following.each do |user| %>
    <div class="col-lg-4">
      <div class="card">
        <%= render('user_card_header', user: user) %>
        <div class="card-body">
          Email:<p class="card-text text-muted"><%= user.email %> </p>
          Phone Number:<p class="card-text text-muted"><%= user.phone_number %></p>
        </div>
        <%= link_to('Unfollow', user_unfollow_path(user), method: :post, class: 'btn btn-secondary') %>
      </div>
    <% end %>
  </div>
